<div class="row">
  <div class="col-md-10">
    <h2>My Links:</h2>

    <%= render @links %>
  </div>
  <div class="col-md-2">
    <h3>My Tags:</h3>

    <% @my_tags.each do |tag| %>
      <ul>
        <li>
          <%= link_to "##{tag.name}", tag_path(tag.name) %>
        </li>
      </ul>
    <% end %>       
  </div>
</div>





